											"parameters": {}
										}
									]
								},
								{
									"name": "call child prod pipeline",
									"type": "ExecutePipeline",
									"dependsOn": [
										{
											"activity": "Copy prod data",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"policy": {
										"secureInput": false
									},
									"userProperties": [],
