							{
								"activity": "fetch Customer record count",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@greater(activity('fetch Customer record count').output.firstRow.rec_count,1000)",
								"type": "Expression"
							},
